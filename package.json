{
  "name": "todoapp_eval_quali-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev --turbopack",
    "dev:server": "cd server && npm run dev",
    "build": "next build && cd server && npm run build",
    "start": "concurrently \"npm run start:next\" \"npm run start:server\"",
    "start:next": "next start",
    "start:server": "cd server && npm start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:headless": "cypress run --headless",
    "test:e2e": "start-server-and-test dev 3000 cypress:run",
    "test:e2e:open": "start-server-and-test dev 3000 cypress:open",
    "test:e2e:ci": "start-server-and-test start 3000 cypress:run",
    "test:all": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.523.0",
    "motion": "^12.19.1",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.25.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
